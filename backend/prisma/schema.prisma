generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  fname String
  lname String
  email String @unique
  password String
  address String
  province String
  district String
  city String
  nic String
  mobile String
}